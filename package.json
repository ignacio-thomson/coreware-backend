{
  "name": "coreware-backend",
  "version": "1.0.0",
  "description": "Coreware application backend developed with Nodejs and Express",
  "main": "index.js",
  "scripts": {
    "swagger": "tsoa spec",
    "nodemon:swagger": "nodemon -x tsoa spec",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "jest",
    "serve:coverage": "npm run test && cd coverage/lcov-report && npx serve",
    "build": "npx webpack --mode development",
    "start": "node dist/index.js",
    "build:prod": "npx webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignacio-thomson/coreware-backend.git"
  },
  "keywords": [
    "express",
    "node",
    "react",
    "jest",
    "swagger",
    "mongoose"
  ],
  "author": "Ignacio Thomson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ignacio-thomson/coreware-backend/issues"
  },
  "homepage": "https://github.com/ignacio-thomson/coreware-backend#readme",
  "dependencies": {
    "@types/express": "^4.17.15",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^4.1.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.15",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "serve": "^14.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin": "^0.5.0"
  }
}
